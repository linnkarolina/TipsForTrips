@model IEnumerable<WebApplicationFinal.Models.MysqlClass>

@{
    ViewBag.Title = "Mysql";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Mysql</h2>

<body>
    

    <table class="table table-hover">
        <thead>
            <tr>
                <th scope="col">ID_admin</th>
                <th scope="col">username</th>
                <th scope="col">password</th>
            </tr>
            @foreach (WebApplicationFinal.Models.MysqlClass details in Model)
            {
                
                <tr class="table">
                    <td>@details.ID_admin</td>
                    <td>@details.Username</td>
                    <td>@details.Password</td>
                </tr>

                }
            </thead>
    </table>
</body>